{
  "manifest_version": 2,
  "name": "Manually sort folders",
  "description": "__MSG_extensionDescription__",
  "version": "2.3.0",
  "author": "Jonathan Protzenko and Itagaki Fumihiko",
  "homepage_url": "https://github.com/protz/Manually-Sort-Folders/wiki",
  "browser_specific_settings": {
    "gecko": {
      "id": "tbsortfolders@xulforum.org",
      "strict_min_version": "68.0",
      "strict_max_version": "103.0"
    }
  },
  "icons": {
    "32": "icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "accountsRead", "storage"
  ],
  "options_ui": {
    "page": "/options/options.html",
    "open_in_tab": true,
    "browser_style": true
  },
  "experiment_apis": {
    "CustomFolderSort": {
      "schema": "api/CustomFolderSort/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "CustomFolderSort"
          ]
        ],
        "script": "api/CustomFolderSort/implementation.js"
      }
    },
    "LegacyPrefs": {
      "schema": "api/LegacyPrefs/schema.json",
      "parent": {
        "scopes": [
          "addon_parent"
        ],
        "paths": [
          [
            "LegacyPrefs"
          ]
        ],
        "script": "api/LegacyPrefs/implementation.js"
      }
    }
  },
  "default_locale": "en"
}
